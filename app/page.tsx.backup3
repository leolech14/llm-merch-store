// Product Card Component - HTML-black.html style (simplified)
interface ProductCardProps {
  name?: string;
  price?: number;
  description?: string;
  images?: string[];
  isNew?: boolean;
  isBestSeller?: boolean;
  isSold?: boolean;
  isSaleActive?: boolean;
  productId?: string;
  category?: string;
  tags?: string[];
}

function ProductCard({
  name = "LLM Science Tee",
  price = 149,
  description = "Limited edition AI-themed design",
  images = ["/Fluffy-Creature-lvls.png"],
  isNew = false,
  isBestSeller = false,
  isSold = false,
  isSaleActive = false,
  productId = "",
  category = "",
  tags = [],
}: ProductCardProps) {
  const [isAddingToCart, setIsAddingToCart] = useState(false);
  const [isAddedToCart, setIsAddedToCart] = useState(false);
  const [isDetailOpen, setIsDetailOpen] = useState(false);

  const handleAddToCart = async () => {
    if (isAddedToCart) return;
    setIsAddingToCart(true);

    // Track add to cart event
    try {
      await fetch('/api/telemetry', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify({
          eventType: 'add_to_cart',
          eventData: { productName: name }
        })
      });
    } catch (error) {
      console.error('Error tracking add to cart:', error);
    }

    setTimeout(() => {
      setIsAddingToCart(false);
      setIsAddedToCart(true);
      setTimeout(() => setIsAddedToCart(false), 2000);
    }, 800);
  };

  return (
    <>
      <Card className="overflow-hidden hover:shadow-xl transition-all border-0 cursor-pointer bg-transparent" onClick={() => setIsDetailOpen(true)} data-product-card>
        <div className="relative">
          <div className="aspect-[4/5] flex items-center justify-center overflow-hidden">
            <img
              src={images[0]}
              alt={name}
              className="w-full h-full object-cover transition-transform hover:scale-105 duration-300"
            />
          </div>
          {/* Badges removed - business model doesn't use them */}
        </div>
      <CardContent className="p-4 space-y-3">
        {/* Single line: Name | Price */}
        <div className="flex items-center justify-between gap-4">
          <h3 className="font-semibold text-sm truncate flex-1">{name}</h3>
          <span className="text-lg font-bold text-foreground whitespace-nowrap">R${price}</span>
        </div>

        {/* Button below */}
        <Button
          className="w-full h-10"
          size="sm"
          onClick={(e) => {
            e.stopPropagation();
            handleAddToCart();
          }}
          disabled={isSold || !isSaleActive || isAddingToCart || isAddedToCart}
          variant={isSold ? "destructive" : "default"}
        >
          {isSold ? (
            "SOLD OUT"
          ) : !isSaleActive ? (
            "Sale Not Active"
          ) : isAddingToCart ? (
            <>
              <Loader2 className="mr-2 h-4 w-4 animate-spin" />
              Adding...
            </>
          ) : isAddedToCart ? (
            <>
              <Check className="mr-2 h-4 w-4" />
              Added!
            </>
          ) : (
            "Quick Buy"
          )}
        </Button>
      </CardContent>
    </Card>

    {/* Product Detail Modal */}
    <ProductDetailModal
      isOpen={isDetailOpen}
      onClose={() => setIsDetailOpen(false)}
      product={{
        name,
        price,
        description,
        images,
        category,
        tags,
        isSold,
        isSaleActive,
      }}
    />
  </>
  );
}

