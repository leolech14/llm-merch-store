  const [showVisitorPopup, setShowVisitorPopup] = useState(false);
  const [stats, setStats] = useState<any>({
    totalPageViews: 1247,
    addToCartEvents: 89,
    totalSales: 156,
    totalProducts: 0,
    totalLikes: 0,
    engagementRate: 18,
    topProducts: []
  });
  const [saleStatus, setSaleStatus] = useState<{
    isActive: boolean;
    status: 'before' | 'during' | 'after';
    startTime: string;
    endTime: string;
    timeUntilStart?: number;
    timeUntilEnd?: number;
  } | null>(null);
  const [inventory, setInventory] = useState<any>(null);
  const [marketPrices, setMarketPrices] = useState<any>(null);
  const [teesLeft, setTeesLeft] = useState(31);

  // Track visitor and fetch all telemetry on page load
  useEffect(() => {
    async function trackAndFetchData() {
      try {
        // Track events in parallel (non-blocking)
        Promise.all([
          fetch('/api/telemetry', {
            method: 'POST',
            headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify({ eventType: 'visitor' })
          }),
          fetch('/api/telemetry', {
            method: 'POST',
            headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify({ eventType: 'page_view' })
          }),
        ]);

        // Fetch ALL data in parallel (FAST!)
        const [visitorData, statsData, saleData, inventoryData, marketData] = await Promise.all([
          fetch('/api/visitors', { method: 'POST' }).then(r => r.json()),
          fetch('/api/stats').then(r => r.json()),
          fetch('/api/sale-status').then(r => r.json()),
          fetch('/api/inventory').then(r => r.json()),
          fetch('/api/market-prices').then(r => r.json()),
        ]);

        // Update all state at once
        const count = visitorData.count;
        setVisitorCount(count);
        setStats(statsData);
        setSaleStatus(saleData);
        setInventory(inventoryData);
        setMarketPrices(marketData);

        // Calculate tees left
        const available = inventoryData.stats?.available || 31;
        setTeesLeft(available);

